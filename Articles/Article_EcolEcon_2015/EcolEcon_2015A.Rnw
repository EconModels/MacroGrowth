                                    "l", "l",
                                    "e", "e", "e", "e")),
    Country = relevelFactor(factor(Country), c("PT", "UK")), # Adjusts facet order
    flavor = relevelFactor(factor(flavor), c("Unadjusted", "Quality-adjusted")), # Adjusts facet order
    variable = relevelFactor(factor(variable), c("y", "k", "l", "e"))
