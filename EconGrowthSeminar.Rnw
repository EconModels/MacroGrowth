% The equation for a single-factor model
\newcommand{\sfEqn}{$y = A \: f^{m}$; \small $A \equiv \mathrm{e}^{\lambda (t-t_0)}$ \normalsize}
% The equation for a cobb-douglas without energy model
\newcommand{\cdEqn}{$y = A \: k^{\alpha}l^{\beta}$; \small $A \equiv \mathrm{e}^{\lambda (t-t_0)}$ \normalsize}
% The equation for a cobb-douglas with energy model
\newcommand{\cdeEqn}{$y = A\: k^{\alpha}l^{\beta}e^{\gamma}$; \small $A \equiv \mathrm{e}^{\lambda (t-t_0)}$ \normalsize}
% The equation for a ces model
\newcommand{\cesEqn}{\scriptsize $y = \gamma \: A \: \left\{\delta \left[\delta_1 k^{-\rho_1} + (1-\delta_1)l^{-\rho_1} \right]^{\rho/\rho_1} + (1-\delta) e^{-\rho} \right\}^{-1/\rho}$; \tiny $A \equiv \mathrm{e}^{\lambda (t-t_0)}$ \normalsize}
% The narrow equation for a ces model
\newcommand{\cesEqnNarrow}{\begin{align*} y = \, &\gamma \: A \\&\times \left\{\delta \left[\delta_1 k^{-\rho_1} + (1-\delta_1)l^{-\rho_1} \right]^{\rho/\rho_1} \right. \\ &\left.+ \; (1-\delta) e^{-\rho} \right\}^{-1/\rho} \end{align*}}
% The equation for a linex model
\newcommand{\linexEqn}{$y =  A \: e$; \small $A \equiv \mathrm{e}^{\left[a_0 \left(2 - \frac{l + e}{k} \right) + a_0 c_t \left(\frac{l}{e} - 1 \right)\right]}$ \normalsize}

% Some commands for parameter tables in slides
\newcommand{\yRow}{$y$ & $Y/Y_0$, output}
\newcommand{\timeRow}{$t$ & year}
\newcommand{\initialTimeRow}{$t_0$ & initial year}
\newcommand{\kRow}{$k$ & $K/K_0$, capital stock}
\newcommand{\lRow}{$l$ & $L/L_0$, labor}
\newcommand{\eRow}{$e$ & $E/E_0$, one of $q$, $x$, or $u$}
\newcommand{\lambdaRow}{$\lambda$ & technological progress}
\newcommand{\alphaRow}{$\alpha$ & ``influence'' of capital}
\newcommand{\betaRow}{$\beta$ & ``influence'' of labor}
\newcommand{\gammaRow}{$\gamma$ & ``influence'' of energy}

