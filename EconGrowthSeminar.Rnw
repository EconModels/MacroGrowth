\subsection[Cobb-Douglas]{Cobb-Douglas} %+++++++++++++++++++++++++++++++++++++++

\minorSeparator{Cobb-Douglas (with Energy)} %----------------------------

\begin{frame}{Parameter Estimation for Cobb-Douglas With Energy} %----------------------------
  \begin{center}\cdeEqn\end{center}
  \begin{itemize}
    \item{Objective: determine $\lambda$, $\alpha$, $\beta$, and $\gamma$ that best fits historical data}
    \item{Perform grid search over $\gamma$: Re-estimate $\lambda$, $\alpha$, and $\beta$ at each value of $\gamma$}
    \item{Respect sum constraint: $\alpha + \beta + \gamma = 1$}
    \item{Respect absolute constraints: $0 \leq \alpha, \beta, \gamma \leq 1$} (not yet)
    \item{$\lambda$, $\alpha$, $\beta$, and $\gamma$ estimated by \texttt{nls} function in \texttt{R}}
  \end{itemize}
\end{frame}

