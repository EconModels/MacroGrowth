% The equation for a single-factor model
\newcommand{\sfEqn}{$y = \mathrm{e}^{\lambda (t-t_0)}f^{m}$}
% The equation for a cobb-douglas without energy model
\newcommand{\cdEqn}{$y = \mathrm{e}^{\lambda (t-t_0)}k^{\alpha}l^{\beta}$}
% The equation for a cobb-douglas with energy model
\newcommand{\cdeEqn}{$y = \mathrm{e}^{\lambda (t-t_0)}k^{\alpha}l^{\beta}e^{\gamma}$}
% The equation for a ces model
\newcommand{\cesEqn}{$y = \gamma \mathrm{e}^{\lambda(t-t_0)} \left\{\delta \left[\delta_1 k^{-\rho_1} + (1-\delta_1)l^{-\rho_1} \right]^{\rho/\rho_1} + (1-\delta) e^{-\rho} \right\}^{-1/\rho}$}
% The equation for a linex model
\newcommand{\linexEqn}{$y = e \: \mathrm{e}^{\left[a_0 \left(2 - \frac{l + e}{k} \right) + a_0 c_t \left(\frac{l}{e} - 1 \right)\right]}$}

% Some commands for parameter tables in slides
\newcommand{\yRow}{$y$ & $Y/Y_0$, output}
\newcommand{\timeRow}{$t$ & year}
\newcommand{\initialTimeRow}{$t_0$ & initial year}
\newcommand{\kRow}{$k$ & $K/K_0$, capital stock}
\newcommand{\lRow}{$l$ & $L/L_0$, labor}
\newcommand{\eRow}{$e$ & $E/E_0$, any of $q$, $x$, or $u$}
\newcommand{\lambdaRow}{$\lambda$ & technological progress}
\newcommand{\alphaRow}{$\alpha$ & ``strength'' of capital}
\newcommand{\betaRow}{$\beta$ & ``strength'' of labor}
\newcommand{\gammaRow}{$\gamma$ & ``strength'' of energy}

% Provides the upper-right location on a page useful for positioning flags on frames
\newcommand{\upperRightFlagLocation}{\{2.14in\}\{1.59in\}}

% Places an image (with scaling) at a position on the page. The origin (0, 0) is 
% assumed to be at the center of the frame.
% Arguments:
%  #1 image path
%  #2 image scaling (1.0 = same size as original image, 2.0 = double size)
%  #3 x offset from center of the frame (include units, if desired. e.g. "1.0in")
%  #4 y offset from center of the frame (include units, if desired. e.g. "1.0in")
\newcommand{\putat}[4]{
  \begin{tikzpicture}[remember picture,overlay] 
    \node [xshift=#3, yshift=#4] at (current page.center){
      \includegraphics[scale=#2]{#1}
    };             
  \end{tikzpicture}
}

% Places a flag image in the upper-right portion of the slide. Assumes that the flag
% file is called flag_<country_abbreviation>.png and is located in 
% a directory called figure_other. This macro calls the \putat macro with location
% x = 2.14 and y = 1.59in.
% Arguments:
%  #1 2-letter country code (lower case)
%  #2 scale factor
\newcommand{\putFlagUpperRight}[2]{
  \putat{figure_other/flag_#1.png}{#2}{2.14in}{1.59in}
}

