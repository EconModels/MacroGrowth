\begin{frame}{Comments on Single-factor Model Equation} %----------------------------
    \begin{center} \sfEqn \end{center}
    \vspace{\baselineskip}
    
    \begin{itemize}
      \item{$\mathrm{e}^{\lambda(t-t_0)}$: Time-dependent technology term}
      \begin{itemize}
        \item{Represents the irrepressible ``progress'' of technology}
        \item{Is independent of factor of production ($f$)}
      \end{itemize}
      \item{Used for illustration purposes here}
      \item{Example: \citet{Nel:2009wq} use $f$ = $q$}
      \item{Parameters estimated by \texttt{nls} function in \texttt{R}}
    \end{itemize}
\end{frame}

\begin{frame}{Curve-fitting as Optimization} %----------------------------
  \begin{itemize}
    \item{Select values of parameters to minimize errors ($SSE$) between predicted and historical economic output ($y$)}
    \vspace{\baselineskip}
    
    \item{Example: Single-factor Model with $Q$}
    \begin{itemize}
      \item{$y = \mathrm{e}^{\lambda (t-t_0)}q^{\gamma}$}
      \item{Select $\lambda$ and $\gamma$ to minimize\\
            $SSE = \sum\limits_{t=1980}^{2011}(y_{pred} - y_{act})^2$}
    \end{itemize}
    \vspace{\baselineskip}
    
    \item{``Curve-fitting,'' ``parameter estimation,''  ``optimization,'' and ``minimization'' are used interchangeably}
  \end{itemize}
\end{frame}

