\subsection[CES]{Evaluation of CES} %+++++++++++++++++++++++++++++++++++++++

\minorSeparator{Constant Elasticity of Substitution (CES)}

\begin{frame}{Constant Elasticity of Substitution (CES)} %----------------------------
  \begin{center}
    \small
      \cesEqn
    \normalsize
    \vspace{\baselineskip}
    
    \begin{tabular}{|c|l|}\hline
      \yRow\\
      \timeRow\\
      \initialTimeRow\\
      \kRow\\
      \lRow\\
      \eRow\\
      \hline
      $\gamma$   &   fitting coefficient\\
      \lambdaRow\\
      $\delta_1$   &   split between ($k$) and ($l$)\\
      $\rho_1$     &   ``influence'' of ($k$) vs. ($l$)\\
      $\delta$   &   split between ($kl$) and ($e$)\\
      $\rho$     &   ``influence'' of ($kl$) vs. ($e$)\\
      \hline
    \end{tabular}
  \end{center}
\end{frame}

\begin{frame}{Comments on CES Model Equation} %----------------------------
  \begin{center} \small \cesEqn \normalsize \end{center}
  \begin{itemize}
    \item{$\mathrm{e}^{\lambda(t-t_0)}$: Time-dependent technology term}
    \item{$\delta$: split between ($kl$) and ($e$)}
    \item{$\sigma \equiv \frac{1}{1+\rho}$: elasticity of substitution ($0 \leq \sigma \leq 1$)}
    \begin{itemize}
      \item{$\sigma \rightarrow 0$: ($kl$) and ($e$) are complementary}
      \item{$\sigma \rightarrow 1$: ($kl$) and ($e$) are substitutes}
    \end{itemize}
    \item{Example: \citet{Stern:2012ey}}
    \item{Parameters estimated by \texttt{cesEst} function in \texttt{R}'s \texttt{micEcon} package}
  \end{itemize}
\end{frame}

